{
    "builds":[
        {
            "src":"build.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        },
    {   "src":"CRM/wsgi.py",
        "use":"@vercel/python",
        "config":{
            "maxLambdaSize":"15mb",
            "runtime":"python3.9"
        }
    }
    
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"CRM/wsgi.py"
        }
    ]
}